{
  "title": "Top 10% of countries with the highest urbanization rates",
  "data": {
  "url": "https://raw.githubusercontent.com/ongkhooi/data-viz-2/main/data/Urbanization%20rate.csv"},
  "encoding": {
    "x": {"field": "Urbanization rate","type": "quantitative", "title":"Urbanization Rate (%)"},
    "y": {"title": "Country", "field": "Country", "type": "nominal"}
  },
  "width": 585,
  "height": 800,
  "mark": "bar",
  "params": [{
    "name": "click",
    "select": {"type": "point", "encodings": ["color"]}
  }],
  "transform": [
    {"filter": "datum['Urbanization rate'] <50"}
  ]
}